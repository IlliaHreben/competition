{
  "name": "competition-server",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "type": "module",
  "scripts": {
    "start": "nodemon --experimental-specifier-resolution=node -r dotenv/config app.js | pino-pretty -c -t",
    "test": "echo \"Error: no test specified\" && exit 1",
    "migration-create": "sequelize-cli migration:generate --name",
    "migration-dev": "NODE_OPTIONS='-r dotenv/config' sequelize db:migrate --config etc/db.cjs",
    "migration-undo": "NODE_OPTIONS='-r dotenv/config' sequelize db:migrate:undo --config etc/db.cjs",
    "migration-undo-all": "NODE_OPTIONS='-r dotenv/config' sequelize db:migrate:undo:all --config etc/db.cjs",
    "seeds-create": "sequelize-cli seed:generate --name",
    "seeds": "NODE_OPTIONS='-r dotenv/config' sequelize db:seed:all --config etc/db.cjs",
    "seeds-undo": "NODE_OPTIONS='-r dotenv/config' sequelize db:seed:undo --config etc/db.cjs",
    "seeds-undo-all": "NODE_OPTIONS='-r dotenv/config' sequelize db:seed:undo:all --config etc/db.cjs",
    "clear-db": "npm run migration-undo-all && npm run migration-dev && npm run seeds"
  },
  "author": "Illia Hreben",
  "license": "ISC",
  "dependencies": {
    "chista": "^1.0.0",
    "cls-hooked": "^4.2.2",
    "cors": "^2.8.5",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "json-pointer": "^0.6.2",
    "livr": "^2.4.0",
    "livr-extra-rules": "^1.3.0",
    "lodash": "^4.17.21",
    "nodemon": "^2.0.20",
    "pg": "^8.8.0",
    "pino": "^6.13.3",
    "pino-http": "^8.2.1",
    "rename-keys": "^2.0.1",
    "sequelize": "^6.25.4",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "babel-register": "^6.26.0",
    "eslint": "^8.27.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-config-standard": "^17.0.0",
    "eslint-plugin-n": "^15.5.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^4.2.1",
    "pino-pretty": "^9.1.1",
    "prettier": "2.7.1",
    "sequelize-cli": "^6.5.2"
  }
}
