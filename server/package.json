{
  "name": "competition-server",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "migration-dev": "env $(cat .env | xargs) sequelize db:migrate  --config etc/db.js",
    "migration-create": "sequelize-cli migration:generate --name -r dotenv/config",
    "seeds": "env $(cat .env | xargs) sequelize db:seed:all --config etc/db.js",
    "seeds-undo": "env $(cat .env | xargs) sequelize db:seed:undo --config etc/db.js",
    "seeds-undo-all": "env $(cat .env | xargs) sequelize db:seed:undo:all --config etc/db.js"
  },
