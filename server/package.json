{
  "name": "competition-server",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "type": "module",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "migration-create": "sequelize-cli migration:generate --name",
    "migration-dev": "NODE_OPTIONS='-r dotenv/config' sequelize db:migrate --config etc/db.js",
    "migration-undo": "NODE_OPTIONS='-r dotenv/config' sequelize db:migrate:undo --config etc/db.js",
    "migration-undo-all": "NODE_OPTIONS='-r dotenv/config' sequelize db:migrate:undo:all --config etc/db.js",
    "seeds-create": "sequelize-cli seed:generate --name",
    "seeds": "NODE_OPTIONS='-r dotenv/config' sequelize db:seed:all --config etc/db.js",
    "seeds-undo": "NODE_OPTIONS='-r dotenv/config' sequelize db:seed:undo --config etc/db.js",
    "seeds-undo-all": "NODE_OPTIONS='-r dotenv/config' sequelize db:seed:undo:all --config etc/db.js",
    "clear-db": "npm run migration-undo-all && npm run migration-dev && npm run seeds"
  },
  "author": "Illia Hreben",
  "license": "ISC",
  "dependencies": {
    "babel-register": "^6.26.0",
    "dotenv": "^10.0.0",
    "eslint": "^7.32.0",
    "eslint-config-standard": "^16.0.3",
    "pg": "^8.7.1",
    "sequelize": "^6.6.5",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "sequelize-cli": "^6.2.0"
  }
}
