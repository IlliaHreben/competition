{
  "name": "competition-server",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "type": "module",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",

    "migration-create": "sequelize-cli migration:generate --name",
    "migration-dev": "sequelize db:migrate --url $(node -r dotenv/config etc/sequelize-db.js)",
    "migration-undo": "sequelize db:migrate:undo --url $(node -r dotenv/config etc/sequelize-db.js)",
    "migration-undo-all": "sequelize db:migrate:undo:all --url $(node -r dotenv/config etc/sequelize-db.js)",

    "seeds-create": "sequelize-cli seed:generate --name",
    "seeds": "sequelize db:seed:all --url $(node -r dotenv/config etc/sequelize-db.js)",
    "seeds-undo": "sequelize db:seed:undo --url $(node -r dotenv/config etc/sequelize-db.js)",
    "seeds-undo-all": "sequelize db:seed:undo:all --url $(node -r dotenv/config etc/sequelize-db.js)"
  },
  "author": "Illia Hreben",
  "license": "ISC",
  "dependencies": {
    "babel-register": "^6.26.0",
    "dotenv": "^10.0.0",
    "eslint": "^7.32.0",
    "eslint-config-standard": "^16.0.3",
    "pg": "^8.7.1",
    "sequelize": "^6.6.5",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "sequelize-cli": "^6.2.0"
  }
}
